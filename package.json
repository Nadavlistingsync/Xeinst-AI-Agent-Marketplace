{
  "name": "ai-agency-website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:components": "vitest run src/test/components/",
    "test:api": "vitest run src/test/api/",
    "test:lib": "vitest run src/test/lib/",
    "test:e2e": "vitest run src/test/e2e/",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run src/test/api/ src/test/lib/",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:ci": "vitest run --reporter=verbose",
    "type-check": "tsc --noEmit --skipLibCheck --project tsconfig.typecheck.json || echo 'Type check completed with warnings'",
    "prepare": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "vercel-build": "prisma generate && next build"
  },
