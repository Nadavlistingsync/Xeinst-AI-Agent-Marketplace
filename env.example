# =============================================================================
# XEINST AI AGENT MARKETPLACE - ENVIRONMENT VARIABLES EXAMPLE
# =============================================================================
# Copy this file to .env.local and update the values as needed
# This file serves as a template for all required environment variables

# =============================================================================
# CORE APPLICATION CONFIGURATION
# =============================================================================
NODE_ENV="development"
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NEXT_PUBLIC_APP_NAME="Xeinst AI Agent Marketplace"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
# PostgreSQL Database URLs - REQUIRED
DATABASE_URL="postgresql://username:password@localhost:5432/xeinst_ai_marketplace"
TEST_DATABASE_URL="postgresql://username:password@localhost:5432/xeinst_ai_marketplace_test"

# Database Connection Pool Settings
DATABASE_POOL_MIN="2"
DATABASE_POOL_MAX="10"

# =============================================================================
# AUTHENTICATION & SECURITY - REQUIRED
# =============================================================================
# NextAuth.js Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-here"

# JWT Configuration
JWT_SECRET="your-jwt-secret-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Security & License
LICENSE_KEY="your-license-key-here"
SECURITY_WATERMARK="AI_AGENCY_2024"

# =============================================================================
# AI SERVICES CONFIGURATION
# =============================================================================
# OpenAI Configuration - Get from https://platform.openai.com/
OPENAI_API_KEY="sk-proj-your-openai-api-key-here"
OPENAI_MODEL="gpt-4"
OPENAI_MAX_TOKENS="4000"
OPENAI_TEMPERATURE="0.7"

# AI Agent Configuration
AI_AGENT_DEFAULT_MODEL="gpt-4"
AI_AGENT_MAX_EXECUTION_TIME="30000"
AI_AGENT_RATE_LIMIT_PER_MINUTE="60"

# Alternative AI Providers (Optional)
# Anthropic Claude
ANTHROPIC_API_KEY=""
ANTHROPIC_MODEL="claude-3-sonnet-20240229"

# Google Gemini
GOOGLE_AI_API_KEY=""
GOOGLE_AI_MODEL="gemini-pro"

# Azure OpenAI
AZURE_OPENAI_API_KEY=""
AZURE_OPENAI_ENDPOINT=""
AZURE_OPENAI_DEPLOYMENT_NAME=""

# =============================================================================
# PAYMENT PROCESSING (STRIPE) - OPTIONAL
# =============================================================================
# Stripe Configuration - Get from https://stripe.com/
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_PUBLISHABLE_KEY="pk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# =============================================================================
# WEBHOOK SYSTEM CONFIGURATION
# =============================================================================
# Core Webhook Settings
ENABLE_WEBHOOKS="true"
WEBHOOK_TIMEOUT="30000"
WEBHOOK_MAX_RETRIES="3"
WEBHOOK_RETRY_DELAY="1000"
WEBHOOK_SECRET="default-webhook-secret"

# Webhook Security
WEBHOOK_SSL_VERIFY="true"
WEBHOOK_USER_AGENT="AI-Agent-Marketplace/1.0"

# Webhook Integrations
ENABLE_N8N_INTEGRATION="true"
ENABLE_ZAPIER_INTEGRATION="true"
ENABLE_MAKE_INTEGRATION="true"

# =============================================================================
# FILE MANAGEMENT
# =============================================================================
# Temporary File Settings
TEMP_FILE_MAX_SIZE="10485760"
TEMP_FILE_RETENTION_HOURS="24"
TEMP_FILE_CLEANUP_INTERVAL="3600000"

# File Retention
FILE_RETENTION_DAYS="1"
EXECUTION_RETENTION_DAYS="30"

# =============================================================================
# AGENT CONFIGURATION
# =============================================================================
# Agent Execution Settings
AGENT_EXECUTION_TIMEOUT="30000"
AGENT_MAX_CONCURRENT_EXECUTIONS="10"
AGENT_MAX_FILE_SIZE="10485760"
AGENT_DEFAULT_PRICE="5"
AGENT_PLATFORM_FEE_PERCENTAGE="20"

# =============================================================================
# RATE LIMITING
# =============================================================================
# General Rate Limiting
RATE_LIMIT_WINDOW_MS="900000"
RATE_LIMIT_MAX_REQUESTS="100"

# API Rate Limiting
API_RATE_LIMIT_WINDOW_MS="60000"
API_RATE_LIMIT_MAX_REQUESTS="60"

# Webhook Rate Limiting
WEBHOOK_RATE_LIMIT_WINDOW_MS="60000"
WEBHOOK_RATE_LIMIT_MAX_REQUESTS="60"

# =============================================================================
# MONITORING & LOGGING
# =============================================================================
# Webhook Monitoring
ENABLE_WEBHOOK_MONITORING="true"
ENABLE_WEBHOOK_ANALYTICS="true"
WEBHOOK_LOG_LEVEL="info"

# Debug Settings
DEBUG_WEBHOOKS="false"
LOG_WEBHOOK_PAYLOADS="false"
MOCK_WEBHOOK_RESPONSES="false"

# Auto Cleanup
ENABLE_AUTO_CLEANUP="true"
CLEANUP_INTERVAL_HOURS="1"

# =============================================================================
# CACHING & PERFORMANCE (REDIS) - OPTIONAL
# =============================================================================
# Redis Configuration - Get from https://upstash.com/
UPSTASH_REDIS_REST_URL=""
UPSTASH_REDIS_REST_TOKEN=""

# =============================================================================
# OAUTH PROVIDERS - OPTIONAL
# =============================================================================
# Google OAuth - Get from https://console.developers.google.com/
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

# GitHub OAuth - Get from https://github.com/settings/developers
GITHUB_CLIENT_ID=""
GITHUB_CLIENT_SECRET=""

# =============================================================================
# EXTERNAL SERVICES - OPTIONAL
# =============================================================================
# Sentry Error Monitoring - Get from https://sentry.io/
NEXT_PUBLIC_SENTRY_DSN=""
SENTRY_AUTH_TOKEN=""
SENTRY_ORG=""
SENTRY_PROJECT=""

# Supabase - Get from https://supabase.com/
NEXT_PUBLIC_SUPABASE_URL=""
SUPABASE_SERVICE_ROLE_KEY=""

# Stack - Get from https://stack.co/
NEXT_PUBLIC_STACK_PROJECT_ID=""
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=""
STACK_SECRET_SERVER_KEY=""

# =============================================================================
# DEPLOYMENT & INFRASTRUCTURE - OPTIONAL
# =============================================================================
# Vercel Configuration - Get from https://vercel.com/
VERCEL_TOKEN=""
VERCEL_ORG_ID=""
VERCEL_PROJECT_ID=""

# WebSocket Configuration
NEXT_PUBLIC_WS_URL="ws://localhost:3000"

# =============================================================================
# CORS & SECURITY
# =============================================================================
# CORS Configuration
CORS_ORIGIN="http://localhost:3000,http://localhost:3001"
CORS_ORIGINS="http://localhost:3000,http://localhost:3001"
ALLOWED_WEBHOOK_ORIGINS="https://n8n.io,https://zapier.com,https://make.com"

# =============================================================================
# NOTIFICATIONS & ALERTS - OPTIONAL
# =============================================================================
# Webhook Failure Notifications
WEBHOOK_FAILURE_NOTIFICATION_EMAIL=""
ENABLE_WEBHOOK_FAILURE_ALERTS="false"

# =============================================================================
# TESTING & DEVELOPMENT
# =============================================================================
# Test Webhook Configuration
TEST_WEBHOOK_URL=""
TEST_WEBHOOK_SECRET=""

# Feature Flags
ENABLE_ANALYTICS="true"
ENABLE_FEEDBACK_SYSTEM="true"
ENABLE_AGENT_MARKETPLACE="true"

# =============================================================================
# DEVELOPMENT TOOLS
# =============================================================================
# Seed User for Development
SEED_USER_EMAIL="admin@xeinst.com"
SEED_USER_PASSWORD="admin123"

# Development Overrides
DEV_SKIP_AUTH="false"
DEV_MOCK_PAYMENTS="false"
DEV_MOCK_WEBHOOKS="false"
